  <persistence-unit name="PAACPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>entity.Producto</class>
    <class>entity.Pais</class>
    <class>entity.DatosLaborales</class>
    <class>entity.Prototipo</class>
    <class>entity.MiembroLgac</class>
    <class>entity.Patente</class>
    <class>entity.ProductoColaborador</class>

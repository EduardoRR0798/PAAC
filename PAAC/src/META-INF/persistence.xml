    <class>entity.Libro</class>
    <class>entity.Participacion</class>
    <class>entity.ProductoProyecto</class>
    <class>entity.Colaborador</class>
    <class>entity.ArticuloIndexado</class>
    <class>entity.Articulo</class>
    <class>entity.Pe</class>
    <class>entity.ProyectoInvestigacionconjunto</class>
    <class>entity.Lgac</class>
    <class>entity.Tesis</class>
    <class>entity.Miembro</class>
    <class>entity.CapituloLibro</class>
    <class>entity.Cuerpoacademicoexterno</class>
    <class>entity.ColaboradorCuerpoacademico</class>
    <class>entity.Proyecto</class>
    <class>entity.Memoria</class>
    <class>entity.CuerpoAcademicoPromep</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/paac?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="ERRProblem"/>
    </properties>
  </persistence-unit>
</persistence>
